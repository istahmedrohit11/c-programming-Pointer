#include <stdio.h>
int main() {
    int a[5],i;
    int *q=a;
    printf("Enter elements:");
    for(i=0;i<5;i++){
        //scanf("%d",&a[i]);
        scanf("%d",a+i);
    }
    printf("The inputs are:");
    for(i=0;i<5;i++){
       // printf("%d ",a[i]);
        printf("%d ",*(q+i));
    }
    return 0;
}

